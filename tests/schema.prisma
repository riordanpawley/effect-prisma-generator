datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

generator effect {
  provider = "node ./dist/index.js"
  output   = "./generated/effect"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

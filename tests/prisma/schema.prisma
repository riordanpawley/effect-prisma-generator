datasource db {
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

generator effect {
  provider         = "node ../dist/index.js"
  output           = "./generated/effect"
  clientImportPath = "../client"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
